<Window x:Class="KeithsFunFunPantry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeithsFunFunPantry"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="1250" Icon="Images/AppIcon.ico">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Pantry" MinWidth="50" Click="PantryView_Click"/>
            <Button Content="Recipe" MinWidth="50" Margin="10,0,0,0" Click="RecipeView_Click"/>
        </StackPanel>
        <Frame x:Name="Frame_HomePage" Margin="0, 100, 0, 0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
